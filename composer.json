{
    "name": "deligoez/speck-cipher",
    "description": "This is my package speck-cipher",
    "keywords": [
        "deligoez",
        "speck-cipher"
    ],
    "homepage": "https://github.com/deligoez/speck-cipher",
    "license": "MIT",
    "authors": [
        {
            "name": "Yunus Emre Delig√∂z",
            "email": "ye@deligoz.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-gmp": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.20",
        "phpbench/phpbench": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Deligoez\\Speck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deligoez\\Speck\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "bench": "vendor/bin/phpbench run tests/Benchmark --report=aggregate --retry-threshold=5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
